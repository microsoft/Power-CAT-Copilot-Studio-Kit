          "actions": {
            "Perform_an_unbound_action:_Compliance_scan": {
              "type": "OpenApiConnection",
              "inputs": {
                "parameters": {
                  "actionName": "cat_ComplianceScan",
                  "item/cat_ComplianceScanLogCloudFlowName": "@if(contains(triggerOutputs()?['body/cat_cloudflowname'],'Scheduler'),'Agent Inventory | Compliance Scan Scheduler','Agent Inventory | Compliance Scan')"
                },
                "host": {
                  "apiId": "/providers/Microsoft.PowerApps/apis/shared_commondataserviceforapps",
                  "operationId": "PerformUnboundAction",
                  "connectionName": "shared_commondataserviceforapps"
