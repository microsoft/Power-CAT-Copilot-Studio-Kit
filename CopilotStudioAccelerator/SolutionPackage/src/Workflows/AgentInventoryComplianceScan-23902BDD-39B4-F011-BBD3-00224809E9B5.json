              },
              "Set_varIsRunCompliance_1": {
                "type": "SetVariable",
                "inputs": {
                  "name": "varIsRunCompliance",
                  "value": "@if(empty(outputs('List_rows:_Check_usage_flow_exists_and_turned_on')?['body/value']), true, false)"
                },
                "runAfter": {
                  "List_rows:_Check_usage_flow_exists_and_turned_on": [
